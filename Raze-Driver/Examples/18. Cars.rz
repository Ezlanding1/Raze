# Example: Cars

trait Vehicle
{
    int capacity;
    string license;

    function Drive();
}

class Car extends Vehicle
{
    function Car(int capacity, string license)
    {
        this.capacity = capacity;
        this.license = license;
    }

    function Drive()
    {
        Print("Driving a car with capacity " + capacity.ToString() + " and license number " + license);
    }
}

class Bus extends Vehicle
{
    function Bus(int capacity, string license)
    {
        this.capacity = capacity;
        this.license = license;
    }

    function Drive()
    {
        Print("Driving a bus with capacity " + capacity.ToString() + " and license number " + license);
    }
}

function Main()
{
    Bus bus = new Bus(50, "ABC123");
    bus.Drive();

    Car car = new Car(6, "DEF456");
    car.Drive();
}
